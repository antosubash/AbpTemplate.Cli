{
    "$schema": "http://json.schemastore.org/template",
    "author": "Anto Subash",
    "classifications": [
        "ABP",
        "C#",
        "dotnet",
        "cli"
    ],
    "identity": "Anto.Cli.Template",
    "sourceName": "AbpTemplate",
    "name": "An .Net Cli Template",
    "description": "An .Net Cli Template",
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "shortName": "cli-template",
    "preferNameDirectory": true,
    "symbols": {
        "api": {
            "type": "parameter",
            "datatype": "string",
            "defaultValue": "your-api-name.com",
            "replaces": "abp.antosubash.com"
        }
    },
    "sources": [
        {
            "modifiers": [
                {
                    "exclude": [
                        "**/[Bb]in/**",
                        "**/[Oo]bj/**",
                        ".template.config/**/*",
                        "**/*.filelist",
                        "**/*.user",
                        "**/*.lock.json"
                    ]
                }
            ]
        }
    ]
}